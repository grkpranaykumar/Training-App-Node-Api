    "passport": "0.3.2",
    "passport-facebook": "2.1.1",
    "passport-google-oauth": "1.0.0",
    "passport-instagram": "1.0.0",
    "passport-local": "1.0.0",
    "passport-twitter": "1.0.4",
    "pm2": "^2.9.1",
    "pug": "2.0.0-beta6",
    "request-ip": "1.2.2",
    "superagent": "^3.4.4",
    "superagent-promise": "^1.1.0",
    "swagger-jsdoc": "^1.3.0",
    "twilio": "2.11.1",
    "validator": "5.5.0",
    "winston": "^2.3.1"
