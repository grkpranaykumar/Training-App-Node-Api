{
  "name": "appbackend",
  "version": "1.0.0",
  "description": "backend for ubmas application",
  "main": "index.js",
  "scripts": {
    "start": "node app.js ",
    "test": " mocha 'test/**/*.js' --timeout 10000  --compilers js:babel-core/register",
    "pretest": "npm run coverage",
    "lint": "eslint server.js",
    "lint-html": "eslint **/*.js -f html -o ./reports/lint-results.html",
    "lint-fix": "eslint --fix **/*.js",
    "deploy": "npm run test && npm run start",
    "test:watch": "npm t -- -w",
    "docs": "apidoc -i app/ -o apidoc/",
    "coverage": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- -R spec test/**/*.js --timeout 10000 --compilers js:babel-core/register"
  },
  "engines": {
    "node": "8.0.0"
  },
  "author": "TARUN",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-node": "^5.2.1",
    "morgan": "1.7.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": true,
    "retainLines": true
  },
  "dependencies": {
    "app-module-path": "1.1.0",
    "axios": "0.14.0",
    "babel-register": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-preset-env": "^1.6.1",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.7",
    "body-parser": "1.15.2",
    "braintree": "1.41.0",
    "censoring": "1.0.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "cors": "2.8.0",
    "csrf": "^3.0.4",
    "csurf": "^1.9.0",
    "email-templates": "2.5.2",
    "express": "4.14.0",
    "express-session": "1.14.1",
    "faker": "3.1.0",
    "helmet": "^3.4.0",
    "immutable": "3.8.1",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "7.1.9",
    "lodash": "4.15.0",
    "marked": "0.3.6",
    "mocha": "^3.2.0",
    "mongoose": "4.5.9",
    "multer": "1.2.0",
    "nodemailer": "2.5.0",
    "nodemailer-mandrill-transport": "1.2.0",
